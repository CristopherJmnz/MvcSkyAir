@model ModelResumenPago
<div class="container">
    <div class="form-pago">
        <label>NumTarjeta</label>
    </div>
    <div class="resumen">
        <ul class="list-group">
            <li class="list-group-item">@Model.Pasajeros</li>
            @foreach (string a in Model.Asientos){
                <li class="list-group-item">@a</li>
            }
            <li class="list-group-item"></li>
            <li class="list-group-item">Clase</li>
            <li class="list-group-item">Asientos</li>
        </ul>
    </div>
</div>  